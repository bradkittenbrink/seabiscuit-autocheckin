<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<solid android:color="#55000000"/>
			<corners android:radius="8dip"/>
	 	</shape>
	</item>
	
	<item>
		<shape>
			<gradient android:startColor="#787878" android:centerColor="#262626" android:endColor="#262626" android:angle="270"  />
			<corners android:radius="8dip"/>
			<stroke android:width="1dip" android:color="#3D3D3D" />	
	 	</shape>
	</item>
	

</selector>